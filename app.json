{
  "expo": {
    "name": "StockApp",
    "slug": "StockApp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5a6868"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pedroOliveira.StockAppbiergartenSmets",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/smets.png",
        "backgroundColor": "#5a6868"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
